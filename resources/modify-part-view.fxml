<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<HBox alignment="CENTER" fillHeight="false" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.inventorymanagementsystem.ModifyPartController">
   <children>
      <AnchorPane fx:id="modifyPartForm" prefHeight="600.0" prefWidth="550.0" HBox.hgrow="NEVER">
