              <Label layoutX="88.0" layoutY="155.0" text="Name" />
              <Label layoutX="88.0" layoutY="193.0" text="Inv" />
              <Label layoutX="88.0" layoutY="234.0" text="Price/Cost" />
              <Label layoutX="88.0" layoutY="270.0" text="Max" />
              <Label fx:id="machineOrName" layoutX="88.0" layoutY="309.0" text="Machine ID" />
              <TextField fx:id="idField" editable="false" layoutX="176.0" layoutY="110.0" prefHeight="25.0" prefWidth="118.0" />
              <TextField fx:id="nameField" layoutX="176.0" layoutY="151.0" prefHeight="25.0" prefWidth="118.0" />
