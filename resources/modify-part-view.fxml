              <TextField fx:id="maxField" layoutX="176.0" layoutY="266.0" prefHeight="25.0" prefWidth="118.0" />
              <TextField fx:id="nameOrMIDField" layoutX="177.0" layoutY="305.0" prefHeight="25.0" prefWidth="118.0" />
              <Label layoutX="310.0" layoutY="270.0" text="Min" />
              <TextField fx:id="minField" layoutX="365.0" layoutY="266.0" prefHeight="25.0" prefWidth="105.0" />
              <Button layoutX="298.0" layoutY="357.0" mnemonicParsing="false" onAction="#onSaveButtonClick" text="Save" />
              <Button cancelButton="true" layoutX="365.0" layoutY="357.0" mnemonicParsing="false" onAction="#onCancelButtonClick" text="Cancel" />
          </children>
      </AnchorPane>
   </children>
</HBox>
